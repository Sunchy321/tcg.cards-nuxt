{
	"name": "nuxt",
	"type": "module",
	"private": true,
	"scripts": {
		"build": "nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "bun run build && wrangler dev",
		"postinstall": "nuxt prepare",
		"deploy": "bun run build && wrangler deploy",
		"cf-typegen": "wrangler types",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"icons:optimize": "bun run icons:icon && bun run icons:symbol",
		"icons:icon": "svgo -rf ./app/assets/icons/icon --config=svgo-icon.config.mjs",
		"icons:symbol": "svgo -rf ./app/assets/icons/symbol --config=svgo-symbol.config.mjs"
	},
	"dependencies": {
		"@nuxt/eslint": "1.15.1",
		"@nuxt/icon": "2.2.1",
		"@nuxt/ui": "4.5.0",
		"@nuxthub/core": "0.10.6",
		"@nuxtjs/i18n": "10.2.3",
		"@orpc/client": "^1.13.5",
		"@orpc/server": "^1.13.5",
		"@orpc/zod": "^1.13.5",
		"@vueuse/core": "^14.2.1",
		"@vueuse/nuxt": "14.2.1",
		"axios": "^1.13.5",
		"better-auth": "^1.4.18",
		"cheerio": "^1.2.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.45.1",
		"lodash": "^4.17.23",
		"lucide-vue-next": "^0.575.0",
		"nuxt": "^4.3.1",
		"postgres": "^3.4.8",
		"reka-ui": "^2.8.2",
		"tailwind-merge": "^3.5.0",
		"vue": "^3.5.28",
		"vue-router": "^4.6.4",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@iconify-json/tabler": "^1.2.27",
		"@types/lodash": "^4.17.24",
		"@types/node": "^25.3.0",
		"drizzle-kit": "^0.31.9",
		"nitro-cloudflare-dev": "^0.2.2",
		"nitropack": "^2.13.1",
		"svgo": "^4.0.0",
		"tailwindcss": "^4.2.0",
		"tw-animate-css": "^1.4.0",
		"typescript": "^5.9.3",
		"wrangler": "^4.67.0"
	}
}
